{
  "name": "ng-universal-demo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "heroku-postbuild": "npm run build:prod:rollup",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "build": "webpack --progress",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client -p & webpack --env.aot --env.server",
    "build:prod:rollup": "npm run build:prod && npm run rollup",
    "build:prod:json": "webpack --env.aot --env.client -p --env.analyzer",
    "rollup": "rollup -c rollup.config.js",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "server:dev": "node dist/server.js",
    "server:dev:watch": "nodemon --debug dist/server.js",
    "start": "nodemon dist/server.js",
    "start:dev": "npm run clean && npm run build && npm run server:dev",
    "watch": "webpack --watch",
    "watch:dev:server": "npm-run-all -p server:dev:watch watch",
    "watch:dev": "npm run clean && npm run build && npm run watch:dev:server",
    "watch:prod:server": "npm-run-all -p server watch",
    "watch:prod": "npm run build:prod:rollup && npm run watch:prod:server"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "next",
    "@angular/app-shell": "^0.1.0",
    "@angular/common": "next",
    "@angular/compiler": "next",
    "@angular/core": "next",
    "@angular/forms": "next",
    "@angular/http": "next",
    "@angular/platform-browser": "next",
    "@angular/platform-browser-dynamic": "next",
    "@angular/platform-server": "next",
    "@angular/router": "next",
    "@angular/service-worker": "next",
    "bulma": "^0.4.0",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "imports-loader": "^0.7.1",
    "moment": "^2.18.1",
    "request": "^2.81.0",
    "rxjs": "^5.2.0",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.5",
    "@angular/compiler-cli": "next",
    "@ngtools/webpack": "next",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.8",
    "autoprefixer": "^6.5.3",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jshashes": "^1.0.6",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sass-loader": "^4.1.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "sw-precache": "^5.1.0",
    "sw-precache-webpack-plugin": "^0.9.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.7",
    "vinyl": "^2.0.1",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-merge": "^4.0.0"
  }
}
